<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>dict.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.dictionary.web.TestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>DictServlet</servlet-name>
        <servlet-class>com.dictionary.web.DictServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DictServlet</servlet-name>
        <url-pattern>/DictServlet</url-pattern>
    </servlet-mapping>
</web-app>